<% provide(:title, 'Log in') %>
<h1>Log in</h1>
<div>
  <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :email %>
      <%= f.text_field :email %>
      <br>
      <%= f.label :password %>
      <%= f.password_field :password %>
      <br>
      <%= f.submit 'Log in' %>
  <% end %>
  <p>New user? <%= link_to 'Sign up', signup_path %></p>
</div>
